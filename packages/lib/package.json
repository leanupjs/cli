    "rollup-plugin-esbuild": "4.3.1",
    "sinon": "11.1.1",
    "typescript": "4.3.2"
  },
  "scripts": {
    "test": "npx cross-env NODE_ENV=test nyc --check-coverage mocha",
    "prepack": "npm test && rm -rf index.* components helpers pattern shares && tsc && rollup -c rollup.config.js",
    "remove": "rm -rf node_modules",
    "reinstall": "npm run remove && npm run update",
    "update": "npm install && npm update && npm outdated"
  },
  "files": [
    "components/**/*.d.ts",
    "components/**/*.js",
    "components/**/*.js.map",
    "helpers/**/*.d.ts",
    "helpers/**/*.js",
    "helpers/**/*.js.map",
    "pattern/**/*.d.ts",
    "pattern/**/*.js",
    "pattern/**/*.js.map",
    "shares/**/*.d.ts",
    "shares/**/*.js",
    "shares/**/*.js.map",
    "umd/**/*.js",
    "umd/**/*.js.map"
  ]
}
