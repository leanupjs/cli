{
  "name": "@leanup/lib",
  "version": "1.2.0-rc.94",
  "description": "This module is a collection of lightweight application features.",
  "author": "Martin Oppitz <npmjs@martinoppitz.com>",
  "homepage": "https://leanupjs.org",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leanupjs/leanup.git"
  },
  "bugs": {
    "url": "https://github.com/leanupjs/leanup/issues"
  },
  "dependencies": {
    "loglevel": "1.7.1"
  },
  "devDependencies": {
    "@types/chai": "4.2.21",
    "@types/loglevel": "1.6.3",
    "@types/node": "16.4.0",
    "@types/sinon": "10.0.2",
    "chai": "4.3.4",
    "cross-env": "7.0.3",
    "esbuild-register": "2.6.0",
    "inferno": "7.4.8",
    "jsdom": "16.6.0",
    "jsdom-global": "3.0.2",
    "mocha": "9.0.2",
    "mock-local-storage": "1.1.17",
    "nyc": "15.1.0",
    "preact": "10.5.14",
    "react": "17.0.2",
    "sinon": "11.1.1",
    "typescript": "4.3.5"
  },
  "scripts": {
    "test": "npx cross-env NODE_ENV=test nyc --check-coverage mocha",
    "prepack": "npm test && rm -rf index.* components helpers pattern shares umd && tsc",
    "remove": "rm -rf node_modules",
    "reinstall": "npm run remove && npm run update",
    "update": "npm install && npm update && npm outdated"
  },
  "files": [
    "components/**/*.d.ts",
    "components/**/*.js",
    "components/**/*.js.map",
    "helpers/**/*.d.ts",
    "helpers/**/*.js",
    "helpers/**/*.js.map",
    "pattern/**/*.d.ts",
    "pattern/**/*.js",
    "pattern/**/*.js.map",
    "shares/**/*.d.ts",
    "shares/**/*.js",
    "shares/**/*.js.map",
    "umd/**/*.js",
    "umd/**/*.js.map"
  ]
}
